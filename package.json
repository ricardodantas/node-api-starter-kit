{
  "name": "node-api-starter-kit",
  "version": "1.0.0",
  "description": "A starter kit to build APIs.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec npm run clusters",
    "start": "npm run clusters",
    "apidoc": "apidoc -i routes/ -o public/apidoc/",
