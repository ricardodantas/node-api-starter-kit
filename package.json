    "compression": "^1.5.1",
    "consign": "^0.1.2",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "helmet": "^1.1.0",
    "jwt-simple": "^0.4.1",
    "lodash": "^4.17.4",
    "morgan": "^1.6.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.0.0",
    "sequelize": "^3.19.2",
    "sqlite3": "^3.1.1",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "apidoc": "^0.15.1",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
